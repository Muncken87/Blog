<% @posts.each do |post| %>
  <div class="post_wrapper">
    <h4 class="title"><%= link_to post.title, post %></h4>
    <p class="date"><%= post.created_at.strftime("%B, %d, %Y") %></p>
    <!-- <%= image_tag post.image_url %> -->
    <%= truncate post.body, length: 200 %>
  </div>
<% end %>
